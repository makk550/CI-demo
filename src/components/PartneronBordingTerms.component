        }
        
        .Referralmodal-content .modal-body ol li,.Referralmodal-content .modal-body p{
              padding: 10px 0px;
            text-align: justify;
            line-height: 150%;
        }
        .Referralmodal-content .modal-body ol {
          list-style-type: none;
          counter-reset: item;
          margin: 0;
          padding: 0;
        }
        
        .Referralmodal-content .modal-body li {
          display: table;
          counter-increment: item;
          margin-bottom: 0.6em;
        }
        
        .Referralmodal-content .modal-body li:before {
            content: counters(item, ".") ". ";
            display: table-cell;
            padding-right: 0.6em;    
        }
        
        .Referralmodal-content .modal-body li li {
            margin: 0;
        }
        
        .Referralmodal-content .modal-body li li:before {
            content: counters(item, ".") " ";
        }
        
        /* Add Animation */
        @-webkit-keyframes animatetop {
            from {top:-300px; opacity:0}
            to {top:0; opacity:1}
        }
        

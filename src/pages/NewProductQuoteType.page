<apex:page controller="QuoteOrderFormController" renderAs="PDF" language="{!LangCode}">
<html>
<head>
<style>
body{font-family: Arial Unicode MS;}
table {page-break-inside:avoid;}
/* Content of Header and Footer */

 @page{ 
 margin : 70pt 10px 10px 1px;             
    @top-center{   
        content:element(header);        
    }
    @bottom-left{
        font-family: Arial Unicode MS;
        font-size: 9px;
        content:"{!FooterString}";
    }
    @bottom-center {
        font-family: Arial Unicode MS;
        font-size: 9px;
        content:"Page"  counter(page) " of " counter(pages);
    }
   
} 
div.header {
    position : running(header) ;
    display: block; page-break-before:avoid;page-break-inside:avoid;
}

.divbreak {display: block; page-break-before:auto; page-break-inside: auto; page-break-after: avoid}
</style>
</head>
